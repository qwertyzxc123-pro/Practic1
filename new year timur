#include <iostream>
using namespace std;
int main() {
    setlocale(0, "");
    cout << "                   \033[33m*\033[0m" << endl;
    cout << "          *       \033[31m/  \\\033[0m      *       " << endl;
    cout << "    *            \033[32m/  \033[31mo \033[32m\\\033[0m             " << endl;
    cout << "                \033[34m/ \033[33m*\033[34m  \033[35m@ \033[34m\\\033[0m        *    " << endl;
    cout << "       *       \033[35m/ \033[36m+      \033[35m\\\033[0m            " << endl;
    cout << "  *           \033[36m/   \033[31mo  \033[33m*   \033[36m\\\033[0m      *    " << endl;
    cout << "             \033[31m/  \033[32m@      \033[34m+  \033[31m\\\033[0m          " << endl;
    cout << "    *       \033[32m/ \033[33m*    \033[35mo     \033[31m@ \033[32m\\\033[0m    *    " << endl;
    cout << "           \033[34m/  \033[36m+  \033[31m*    \033[33m@     \033[34m\\\033[0m        " << endl;
    cout << "  *       \033[35m/ \033[32m@      \033[33mo    \033[36m+    \033[35m\\\033[0m    *  " << endl;
    cout << "         \033[36m/____________________\\\033[0m" << endl;
    cout << "            \033[35m[#]\033[0m  \033[31m|    |\033[0m  \033[33m[+]\033[0m" << endl;
    cout << "         \033[36m[  $  ]\033[0m \033[31m|____|\033[32m[  @  ]\033[0m" << endl;
    cout << "\033[34mC \033[0m \033[35mновым \033[0m \033[36m2026 \033[0m \033[33mгодом! \033[0m" << endl;


    return 0;
} 
